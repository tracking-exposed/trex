    "classnames": "^2.3.1",
    "create-react-class": "^15.7.0",
    "immutability-helper": "^2.9.1",
    "jquery": "^3.6.0",
    "lodash": "^4.17.21",
    "marked": "^0.8.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
