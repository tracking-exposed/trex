{
  "name": "yttrex-extension",
  "version": "2.2.0",
  "description": "yttrex is the extension + backend for youtube algorithm analysis. This is part of the https://tracking.exposed umbrella project",
  "homepage": "https://youtube.tracking.exposed",
  "bugs": "https://github.com/tracking-exposed/yttrex/issues",
  "license": "AGPL-3.0",
  "author": "Claudio Agosti, Alberto Granzotto, and the tracking exposed team",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tracking-exposed/yttrex.git"
  },
  "scripts": {
    "clean": "rm -rf ./build ./dist",
    "lint": "eslint ./ --quiet",
    "start": "npm run build:watch:files",
    "build": "rm -rf ./build && mkdir build && cross-env webpack && cp -r public/* ./build/",
    "build:watch": "rm -rf ./build && mkdir build && cp -r public/* ./build/ && cross-env webpack -w",
    "dist": "sh pack-extension.sh",
    "dist:guardoni": "sh pack-extension-for-guardoni.sh",
    "test": "mocha --compilers js:babel-register --reporter spec",
    "test:watch": "mocha -w --compilers js:babel-register --reporter spec"
  },
  "devDependencies": {
    "@types/mini-css-extract-plugin": "^2.5.1",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "autoprefixer": "^6.7.7",
    "copy-webpack-plugin": "^9.1.0",
    "cross-env": "^2.0.1",
    "css-loader": "^6.6.0",
    "dotenv": "^14.3.2",
    "eslint": "^8.8.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.28.0",
    "glob": "^7.2.0",
    "mini-css-extract-plugin": "^2.5.3",
    "mocha": "^7.2.0",
    "postcss-loader": "^0.10.1",
    "react-transform-hmr": "^1.0.4",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.68.0",
    "webpack-combine-loaders": "^2.0.4",
    "webpack-dev-server": "^4.7.4",
    "webpack-notifier": "^1.15.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "bs58": "^3.1.0",
    "cheerio": "^0.22.0",
    "classnames": "^2.3.1",
    "create-react-class": "^15.7.0",
    "html2canvas": "^1.4.1",
    "immutability-helper": "^2.9.1",
    "jquery": "^3.6.0",
    "lodash": "^4.17.21",
    "marked": "^0.8.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "tweetnacl": "^0.14.5",
    "tweetnacl-util": "^0.13.5",
    "uuid": "^2.0.3"
  }
}
