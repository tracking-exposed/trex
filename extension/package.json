{
  "name": "tktrex",
  "version": "0.2.0",
  "description": "tktrex is the extension + backend for tiktok algorithm analysis. This is part of the https://tracking.exposed umbrella project",
  "homepage": "https://tiktok.tracking.exposed",
  "bugs": "https://github.com/tracking-exposed/tktrex/issues",
  "license": "AGPL-3.0",
  "author": "Tracking Exposed team",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tracking-exposed/tktrex.git"
  },
  "scripts": {
    "lint": "eslint ./ --quiet",
    "build": "rm -rf ./build && mkdir build && cross-env webpack && cp -r src/popup/* manifest.json icons/* ./build/",
    "start": "npm run build:watch:files",
    "build:watch": "rimraf ./build && mkdir build && cp -r manifest.json icons/* src/popup/* ./build/ && cross-env webpack -w",
    "build:dist": "sh pack-extension.sh",
    "clean": "rm -rf ./build ./dist",
    "test": "mocha --compilers js:babel-register --reporter spec",
    "test:watch": "mocha -w --compilers js:babel-register --reporter spec"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^6.1.2",
