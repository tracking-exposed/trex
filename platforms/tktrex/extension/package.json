{
  "name": "@tktrex/extension",
  "version": "2.6.2",
  "packageManager": "yarn@3.1.1",
  "scripts": {
    "clean": "rm -rf build dist",
    "lint": "eslint ./src --quiet",
    "build": "DEBUG=\"*\" webpack",
    "build:guardoni": "BUILD_TARGET=guardoni DEBUG=\"*\" webpack",
    "dist": " DEBUG=\"*:info,*:error\" NODE_ENV=production webpack",
    "dist:guardoni": "DEBUG=\"*:info,*:error\" NODE_ENV=production BUILD_TARGET=guardoni yarn build",
    "watch": "DOTENV_CONFIG_PATH=.env.development webpack --watch",
    "watch:prod": "webpack --watch",
    "test": "jest"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@types/lodash": "^4.14.182",
    "debug": "^4.3.4",
    "dotenv": "^16.0.1",
    "fp-ts": "^2.11.9",
    "jquery": "^3.6.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "nacl": "^0.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/jquery": "^3.5.14",
    "@types/node": "^16.11.36",
    "@types/react": "^17.0.47",
    "@types/react-dom": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "io-ts": "^2.2.16",
    "jest": "^27.5.1",
    "jest-environment-jsdom-global": "^3.1.2",
    "jest-fetch-mock": "^3.0.3",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0"
  }
}
