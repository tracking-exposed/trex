{
  "name": "guardoni",
  "version": "2.5.4",
  "description": "Guardoni is a tool to automate access and coordinate personalization algorithm analysis",
  "scripts": {
    "clean": "rm -rf build dist",
    "pkg": "sh scripts/pkg.sh",
    "lint": "eslint ./src",
    "format": "prettier -w ./src",
    "test": "jest",
    "build": "webpack",
    "watch": "NODE_ENV=development webpack watch",
    "dev": "DOTENV_CONFIG_PATH=../../.env.development electron ./reload.js",
    "start": "electron .",
    "pack": "electron-builder build --dir",
    "dist:linux": "electron-builder build -l",
    "dist:mac": "electron-builder build -m",
    "dist:windows": "electron-builder build -w",
    "dist:all": "scripts/dist-all.sh",
    "postinstall": "electron-builder install-app-deps",
    "prepack": "yarn build",
    "check-stealth": "ts-node ./bin/check-stealth.ts",
    "tdd": "jest --watch --verbose"
  },
  "bin": {
    "cli": "./bin/guardoni-cli.js"
  },
  "main": "./build/electron/main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tracking-exposed/yttrex.git"
  },
  "keywords": [
    "youtube",
    "algorithm",
    "analysis"
  ],
  "author": {
    "name": "The Tracking Exposed team https://tracking.exposed",
    "email": "team@tracking.exposed"
  },
  "license": "AGPL-3.0-or-later",
  "homepage": "https://youtube.tracking.exposed/guardoni",
  "dependencies": {
    "@dicebear/avatars": "^4.10.2",
    "@dicebear/avatars-avataaars-sprites": "^4.10.2",
    "@dicebear/avatars-bottts-sprites": "^4.10.2",
    "@electron/remote": "^2.0.8",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.61",
    "axios": "^0.24.0",
    "csv-parse": "^4.16.3",
    "csv-stringify": "^6.0.5",
    "debug": "^4.3.4",
    "electron-debug": "^3.2.0",
    "electron-log": "^4.4.6",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.1",
    "electron-unhandled": "^4.0.1",
    "extract-zip": "^2.0.1",
    "fp-ts": "^2.11.9",
    "io-ts": "^2.2.16",
    "io-ts-types": "^0.5.16",
    "leveldown": "^5.6.0",
    "linkedom": "^0.14.4",
    "lodash": "^4.17.21",
    "nconf": "^0.11.3",
    "node-fetch": "^2.6.7",
    "puppeteer-core": "^13.7.0",
    "puppeteer-extra": "^3.2.3",
    "puppeteer-extra-plugin-stealth": "^2.9.0",
    "puppeteer-in-electron": "^3.0.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "unzipper": "^0.10.11",
    "ws": "^8.5.0",
    "yaml": "^1.10.2",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@types/lodash": "^4.14.182",
    "@types/nconf": "^0.10.2",
    "@types/prettier": "^2.6.3",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@types/unzipper": "^0.10.5",
    "@types/webpack-node-externals": "^2.5.3",
    "@types/ws": "^8.5.3",
    "@types/yaml": "^1.9.7",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "bufferutil": "^4.0.6",
    "canvas": "^2.9.1",
    "chalk": "^5.0.0",
    "electron": "16.2.4",
    "electron-builder": "^23.0.3",
    "electron-reloader": "^1.2.3",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "jest": "^27.5.1",
    "node-loader": "^2.0.0",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "pkg": "^5.7.0",
    "prettier": "^2.6.2",
    "rpmbuild": "^0.0.23",
    "string-replace-loader": "^3.1.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.1.5",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.7.2",
    "unlazy-loader": "^0.1.3",
    "utf-8-validate": "^5.0.9",
    "webpack": "^5.72.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  },
  "pkg": {
    "scripts": [
      "bin/guardoni-cli.js",
      "../../node_modules/puppeteer/lib/*.js",
      "../../node_modules/puppeteer-extra-plugin-stealth/**/*.js"
    ],
    "assets": [
      "static/index.html",
      "config/default.json",
      "package.json"
    ],
    "targets": [
      "node16-linux-x64",
      "node16-win-x64",
      "node16-macos-x64"
    ],
    "outputPath": "dist"
  }
}
