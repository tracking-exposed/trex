{
  "name": "@yttrex/backend",
  "version": "2.5.4",
  "description": "youtube.tracking.exposed - backend data collector",
  "private": true,
  "scripts": {
    "build": "tsc -b",
    "watch": "key=fuffa DEBUG=\"*,-express:*,-body-parser:*,-follow-redirects\" ts-node-dev -r tsconfig-paths/register --respawn --transpile-only bin/server",
    "old_start_old": "log=$(bash -c 'git log -30 --stat --oneline --decorate --graph') DEBUG=\"*,-body-parser:*,-express:*,-lib:*,-follow-redirects\" ts-node -r tsconfig-paths/register bin/server",
    "start": "DEBUG=\"*,-body-parser:*,-express:*,-lib:*,-follow-redirects\" ts-node bin/server",
    "parserv": "DEBUG=\"*,-pubtimeAPI\" ts-node bin/parser.ts",
    "parserv:watch": "DEBUG=\"*,-pubtimeAPI\" ts-node-dev -r tsconfig-paths/register --respawn --transpile-only bin/parser.ts",
    "leaveserv": "DEBUG=\"*\" ts-node bin/leaves-parser.ts",
    "leaveserv:watch": "DEBUG=\"*\" ts-node-dev -r tsconfig-paths/register --respawn --transpile-only bin/leaves-parser.ts",
    "test": "jest",
    "campaigns": "node bin/campaignUpdater.js --dest https://youtube.tracking.exposed",
    "type-check": "tsc",
    "lint": "eslint ./bin ./lib ./models ./parsers ./routes"
  },
  "author": "Claudio Agosti <claudio@tracking.exposed> and https://github.com/tracking-exposed/yttrex/graphs/contributors",
  "license": "AGPL-3.0",
  "dependencies": {
    "abort-controller": "^3.0.0",
    "body-parser": "^1.19.1",
    "bs58": "^3.1.0",
    "chardet": "^1.4.0",
    "cookie": "^0.3.1",
    "cors": "^2.8.5",
    "date-fns": "^2.28.0",
    "debug": "^4.3.4",
    "express": "^4.17.2",
    "fetch-opengraph": "^1.0.35",
    "food-words": "^1.1.0",
    "fp-ts": "^2.11.9",
    "io-ts": "^2.2.16",
    "io-ts-types": "^0.5.16",
    "jsdom": "^16.7.0",
    "moment": "^2.29.2",
    "mongodb": "^4.3.1",
    "nacl-signature": "^1.0.0",
    "nconf": "^0.8.5",
    "node-fetch": "^2.6.7",
    "numeral": "^2.0.6",
    "ts-endpoint": "^2.0.0",
    "ts-io-error": "^2.0.0",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "tweetnacl": "^0.14.5"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/jest": "^27.5.1",
    "@types/jsdom": "^16.2.14",
    "@types/nconf": "^0.10.2",
    "@types/node": "^16.11.26",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.5",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.7.2"
  }
}
