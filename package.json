  },
  "scripts": {
    "clean": "rm -rf ./build ./dist",
    "lint": "eslint ./src --quiet",
    "build": "rm -rf ./build && mkdir build && cross-env webpack && cp -r src/embedded/* manifest.json icons/* ./build/",
    "build:watch": "rimraf ./build && mkdir build && cp -r src/embedded/* manifest.json icons/* ./build/ && cross-env webpack -w",
    "build:dist": "sh pack-extension.sh",
    "start": "npm run build:watch",
    "dashboard:start": "SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "test": "mocha --compilers js:babel-register --reporter spec",
    "test:watch": "mocha -w --compilers js:babel-register --reporter spec"
  },
