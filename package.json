{
  "name": "yttrex",
  "version": "1.1.2",
  "description": "yttrex is the extension + backend for youtube algorithm analysis. This is part of the https://tracking.exposed plan",
  "homepage": "https://github.com/tracking-exposed/yttrex/",
  "bugs": "https://github.com/tracking-exposed/yttrex/issues",
  "license": "GPL-3.0",
  "author": "Claudio Agosti",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tracking-exposed/yttrex.git"
  },
  "scripts": {
    "lint": "eslint ./",
    "build": "rimraf ./build && cross-env webpack && cp -r src/popup/* manifest.json icons/* ./build/",
    "start": "npm run build:watch:files",
    "build:watch": "cross-env node dev-server.js",
    "build:watch:files": "rimraf ./build && mkdir build && cp -r manifest.json icons/* src/popup/* ./build/ && cross-env webpack -w",
    "build:dist": "./pack-extension.sh || true",
    "clean": "rimraf ./build ./dist",
    "test": "mocha --compilers js:babel-register --reporter spec",
    "test:watch": "mocha -w --compilers js:babel-register --reporter spec",
    "docs": "rimraf ./docs && ./build-docs.sh"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.4.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-display-name": "^6.25.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-no-commonjs": "0.0.2",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "chai": "^3.5.0",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "docco": "^0.7.0",
    "dotenv": "^2.0.0",
    "eslint": "^5.6.0",
    "eslint-config-ascribe-react": "^1.4.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^2.3.1",
    "extract-text-webpack-plugin": "=2.0.0-beta.1",
    "glob": "^7.1.3",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.2",
    "postcss-loader": "^0.10.1",
    "react-transform-hmr": "^1.0.4",
    "rimraf": "^2.6.2",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.2",
    "webpack": "=2.1.0-beta.17",
    "webpack-combine-loaders": "^2.0.4",
    "webpack-dev-server": "^3.1.8",
    "webpack-notifier": "^1.6.0",
    "write-file-webpack-plugin": "^3.3.0"
  },
  "dependencies": {
    "bs58": "^3.0.0",
    "cheerio": "^0.22.0",
    "classnames": "^2.2.6",
    "core-js": "^2.5.7",
    "immutability-helper": "^2.8.1",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "marked": "^0.4.0",
    "material-ui": "^0.16.7",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "tweetnacl": "^0.14.3",
    "tweetnacl-util": "^0.13.3",
    "uuid": "^2.0.3"
  }
}
