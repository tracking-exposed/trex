{
  "name": "yttrex-backend",
  "version": "1.7.5",
  "description": "youtube.tracking.exposed - backend data collector",
  "scripts": {
    "watch": "key=fuffa DEBUG=*,-express:*,-body-parser:*,-follow-redirects node_modules/.bin/ts-node-dev -r tsconfig-paths/register --respawn --transpile-only bin/server.js",
    "start": "gitlog=`git log -30 --stat --oneline --decorate --graph` DEBUG=*,-body-parser:*,-express:*,-lib:*,-follow-redirects ts-node -r tsconfig-paths/register bin/server",
    "parserv": "DEBUG=*,-pubtimeAPI node_modules/.bin/ts-node bin/parserv2.js",
    "parserv:watch": "DEBUG=*,-pubtimeAPI node_modules/.bin/ts-node-dev --respawn --transpile-only bin/parserv2.js",
    "leaveserv": "DEBUG=* node bin/leaveserv.js",
    "leaveserv:watch": "DEBUG=* node_modules/.bin/ts-node-dev --respawn --transpile-only bin/leaveserv.js",
    "test": "mocha --require ts-node/register tests/basic/*",
    "campaigns": "node bin/campaignUpdater.js --dest https://youtube.tracking.exposed",
    "type-check": "tsc"
  },
  "author": "Claudio Agosti <claudio@tracking.exposed> and https://github.com/tracking-exposed/yttrex/graphs/contributors",
  "license": "AGPL-3.0",
  "dependencies": {
    "body-parser": "^1.15.2",
    "bs58": "^3.1.0",
    "chardet": "^1.4.0",
    "cookie": "^0.3.1",
    "cors": "^2.8.1",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "fetch-opengraph": "^1.0.30",
    "food-words": "^1.1.0",
    "fp-ts": "^2.11.5",
    "io-ts": "^2.2.16",
    "io-ts-types": "^0.5.16",
    "jsdom": "^13.0.0",
    "moment": "^2.29.1",
    "mongodb": "^4.1.4",
    "nacl-signature": "^1.0.0",
    "nconf": "^0.8.4",
    "node-fetch": "^2.6.1",
    "node-libcurl": "^2.3.3",
    "numeral": "^2.0.6",
    "request": "^2.88.2",
    "ts-endpoint": "^2.0.0",
    "ts-io-error": "^2.0.0",
    "tweetnacl": "^0.14.3"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "mocha": "^9.1.3",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.4.4"
  }
}
