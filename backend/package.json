{
  "name": "yttrex-backend",
  "version": "1.7.5",
  "description": "youtube.tracking.exposed - backend data collector",
  "scripts": {
    "watch": "key=fuffa DEBUG=*,-express:*,-body-parser:*,-follow-redirects node_modules/.bin/ts-node-dev -r tsconfig-paths/register --respawn --transpile-only bin/server",
    "start": "gitlog=`git log -30 --stat --oneline --decorate --graph` DEBUG=*,-body-parser:*,-express:*,-lib:*,-follow-redirects ts-node -r tsconfig-paths/register bin/server",
    "parserv": "DEBUG=*,-pubtimeAPI node_modules/.bin/ts-node bin/parserv2.js",
    "parserv:watch": "DEBUG=*,-pubtimeAPI node_modules/.bin/ts-node-dev --respawn --transpile-only bin/parserv2.js",
    "leaveserv": "DEBUG=* node bin/leaveserv.js",
    "leaveserv:watch": "DEBUG=* node_modules/.bin/ts-node-dev --respawn --transpile-only bin/leaveserv.js",
    "test": "jest",
    "campaigns": "node bin/campaignUpdater.js --dest https://youtube.tracking.exposed",
    "type-check": "tsc",
    "lint": "eslint ./"
  },
  "author": "Claudio Agosti <claudio@tracking.exposed> and https://github.com/tracking-exposed/yttrex/graphs/contributors",
  "license": "AGPL-3.0",
  "dependencies": {
    "body-parser": "^1.15.2",
    "bs58": "^3.1.0",
    "chardet": "^1.4.0",
    "cookie": "^0.3.1",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "fetch-opengraph": "^1.0.30",
    "food-words": "^1.1.0",
    "fp-ts": "^2.11.5",
    "io-ts": "^2.2.16",
    "io-ts-types": "^0.5.16",
    "jsdom": "^13.0.0",
    "moment": "^2.29.1",
    "mongodb": "^4.1.4",
    "nacl-signature": "^1.0.0",
    "nconf": "^0.8.4",
    "node-fetch": "^2.6.1",
    "node-libcurl": "^2.3.3",
    "numeral": "^2.0.6",
    "request": "^2.88.2",
    "ts-endpoint": "^2.0.0",
    "ts-io-error": "^2.0.0",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "debug": "^4.3.2",
    "tweetnacl": "^0.14.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/jest": "^27.0.3",
    "@types/nconf": "^0.10.1",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "jest": "^27.3.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.4.4"
  }
}
