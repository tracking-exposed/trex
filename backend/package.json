{
  "name": "yttrex-backend",
  "version": "1.5.0",
  "description": "youtube.tracking.exposed - backend data collector",
  "scripts": {
    "watch": "key=fuffa DEBUG=*,-body-parser*,-express*,-follow-redirects nodemon --config config/nodemon.json bin/server",
    "start": "DEBUG=*,-body-parser:*,-express:*,-lib:mongo node bin/server",
    "start:ts": "DEBUG=*,-body-parser:*,-express:*,-lib:mongo ts-node bin/server",
    "parserv": "DEBUG=*,-parser:home:warning node bin/parserv2.js",
    "searches": "DEBUG=* node bin/searches.js",
    "t:lib": "mocha tests/lib/*",
    "test": "mocha tests/basic/*",
    "campaigns": "node bin/campaignUpdater.js --dest https://youtube.tracking.exposed"
  },
  "author": "Claudio Agosti <claudio@tracking.exposed> and https://github.com/tracking-exposed/yttrex/graphs/contributors",
  "license": "AGPL-3.0",
  "dependencies": {
    "bluebird": "^3.7.2",
    "body-parser": "^1.15.2",
    "bs58": "^3.1.0",
    "chardet": "^1.3.0",
    "cookie": "^0.3.1",
    "cors": "^2.8.1",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "fetch-opengraph": "^1.0.30",
    "food-words": "^1.1.0",
    "io-ts": "^2.2.16",
    "jsdom": "^13.0.0",
    "moment": "^2.29.1",
    "mongodb3": "npm:mongodb@3.2.3",
    "nacl-signature": "^1.0.0",
    "nconf": "^0.8.4",
    "node-fetch": "^2.6.1",
    "node-libcurl": "^2.3.3",
    "nodemon": "^2.0.12",
    "numeral": "^2.0.6",
    "request": "^2.88.2",
    "ts-endpoint": "^2.0.0",
    "tweetnacl": "^0.14.3"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "mocha": "^9.1.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
