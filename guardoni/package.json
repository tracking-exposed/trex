{
  "name": "guardoni",
  "version": "2.3.0",
  "description": "Guardoni is a tool to automate access and coordinate personalization algorithm analysis",
  "scripts": {
    "clean": "rm -rf build dist",
    "pkg": "sh scripts/pkg.sh",
    "lint": "eslint ./src/**",
    "format": "prettier -w ./src",
    "test": "jest",
    "build": "webpack",
    "watch": "NODE_ENV=development webpack watch",
    "dev": "DEBUG=\"guardoni*\" NODE_ENV=development electron ./reload.js",
    "start": "electron .",
    "pack": "electron-builder build --dir",
    "dist:linux": "electron-builder build -l",
    "dist:mac": "electron-builder build -m",
    "dist:windows": "electron-builder build -w",
    "dist:all": "scripts/dist-all.sh",
    "postinstall": "electron-builder install-app-deps",
    "prepack": "yarn build"
  },
  "bin": "./bin/guardoni-cli.js",
  "main": "./build/electron/main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tracking-exposed/yttrex.git"
  },
  "keywords": [
    "youtube",
    "algorithm",
    "analysis"
  ],
  "author": {
    "name": "The Tracking Exposed team https://tracking.exposed",
    "email": "team@tracking.exposed"
  },
  "license": "AGPL-3.0-or-later",
  "homepage": "https://youtube.tracking.exposed/guardoni",
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "axios": "^0.24.0",
    "csv-parse": "^4.16.3",
    "csv-stringify": "^6.0.5",
    "debug": "^4.3.3",
    "electron-debug": "^3.2.0",
    "electron-log": "^4.4.6",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.1",
    "fp-ts": "^2.11.8",
    "io-ts": "^2.2.16",
    "io-ts-types": "^0.5.16",
    "lodash": "^4.17.21",
    "nconf": "^0.11.3",
    "node-fetch": "^2.6.7",
    "puppeteer-core": "^13.4.0",
    "puppeteer-extra": "^3.2.3",
    "puppeteer-extra-plugin-stealth": "^2.9.0",
    "puppeteer-in-electron": "^3.0.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "@types/lodash": "^4.14.178",
    "@types/nconf": "^0.10.1",
    "@types/prettier": "^2.4.3",
    "@types/react": "17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/webpack-node-externals": "^2.5.3",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "bufferutil": "^4.0.6",
    "chalk": "^5.0.0",
    "electron": "16.0.8",
    "electron-builder": "^22.14.13",
    "electron-reloader": "^1.2.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "jest": "^27.4.7",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "pkg": "^5.5.2",
    "prettier": "^2.5.1",
    "rpmbuild": "^0.0.23",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "utf-8-validate": "^5.0.8",
    "webpack": "^5.69.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  },
  "pkg": {
    "scripts": [
      "./bin/guardoni-cli.js",
      "./build/guardoni/**/*.js"
    ],
    "assets": [
      "extension/.keep",
      "screenshots/.keep",
      "experiments/.keep",
      "static/index.html",
      "config/default.json",
      "profiles/.keep",
      "package.json"
    ],
    "targets": [
      "node16-linux-x64",
      "node16-win-x64",
      "node16-macos-x64"
    ],
    "outputPath": "dist"
  }
}
