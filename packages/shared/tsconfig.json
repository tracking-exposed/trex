{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "noEmit": false,
    "composite": true,
    "lib": ["ESNext", "DOM"],
    "typeRoots": ["../../node_modules/@types", "./node_modules/@types"],
    "outDir": "./build",
    "baseUrl": "./",
    "rootDir": "./src",
    "allowJs": true,
    "tsBuildInfoFile": "./build/.tsbuildinfo",
    "esModuleInterop": true,
    "jsx": "react"
  },
  "include": ["./src"]
}
