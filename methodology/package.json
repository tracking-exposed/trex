{
  "name": "guardoni",
  "version": "1.9.0",
  "description": "Guardoni is a tool to automate access and coordinate personalization algorithm analysis",
  "scripts": {
    "clean": "rm -rf build/**",
    "pkg": "pkg ./package.json && sh ./bin/post-compile.sh",
    "lint": "eslint ./src",
    "format": "prettier -w ./src",
    "build": "webpack",
    "watch": "webpack watch",
    "dev": "electron ./reload.js",
    "start": "electron .",
    "pack": "electron-builder build --dir",
    "dist:win": "electron-builder build -w",
    "dist": "electron-builder build -l",
    "postinstall": "electron-builder install-app-deps"
  },
  "bin": "./bin/guardoni-cli.js",
  "main": "./build/desktop/main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tracking-exposed/yttrex.git"
  },
  "keywords": [
    "youtube",
    "algorithm",
    "analysis"
  ],
  "author": {
    "name": "Claudio Agosti and the TRex team",
    "email": "team@tracking.exposed"
  },
  "license": "AGPL-3.0-or-later",
  "homepage": "https://youtube.tracking.exposed/guardoni",
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "axios": "^0.24.0",
    "csv-parse": "^4.16.3",
    "debug": "^4.3.3",
    "electron-debug": "^3.2.0",
    "electron-log": "^4.4.4",
    "electron-puppeteer": "^1.1.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.1",
    "fp-ts": "^2.11.5",
    "io-ts": "^2.2.16",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "nconf": "^0.11.3",
    "node-fetch": "^2.6.6",
    "puppeteer": "^10.4.0",
    "puppeteer-core": "^13.0.1",
    "puppeteer-extra": "^3.2.3",
    "puppeteer-extra-plugin-stealth": "^2.9.0",
    "puppeteer-in-electron": "^3.0.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "@types/eslint": "^8.2.1",
    "@types/lodash": "^4.14.178",
    "@types/nconf": "^0.10.1",
    "@types/prettier": "^2",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/webpack-node-externals": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "bufferutil": "^4.0.5",
    "electron": "^16.0.5",
    "electron-builder": "^22.14.5",
    "electron-reloader": "^1.2.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "pkg": "^5.5.1",
    "prettier": "^2.5.1",
    "rpmbuild": "^0.0.23",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "utf-8-validate": "^5.0.7",
    "webpack": "^5.65.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  },
  "pkg": {
    "scripts": [
      "./bin/guardoni-cli.js",
      "./build/guardoni/**/*.js"
    ],
    "assets": [
      "extension/.keep",
      "screenshots/.keep",
      "experiments/.keep",
      "static/index.html",
      "config/default.json",
      "profiles/.keep"
    ],
    "targets": [
      "node14-linux-x64",
      "node14-win-x64",
      "node14-macos-x64"
    ],
    "outputPath": "dist"
  }
}
