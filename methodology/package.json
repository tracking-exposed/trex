{
  "name": "guardoni",
  "version": "1.8.7",
  "description": "Guardoni is a tool to automate access and coordinate personalization algorithm analysis",
  "scripts": {
    "pkg": "pkg ./package.json && sh ./bin/post-compile.sh"
  },
  "bin": "./bin/guardoni.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tracking-exposed/yttrex.git"
  },
  "keywords": [
    "youtube",
    "algorithm",
    "analysis"
  ],
  "author": "Claudio Agosti and the TRex team",
  "license": "AGPL-3.0-or-later",
  "homepage": "https://youtube.tracking.exposed/guardoni",
  "dependencies": {
    "csv-parse": "^4.15.3",
    "debug": "^4.3.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "nconf": "^0.11.3",
    "node-fetch": "^2.6.6",
    "puppeteer": "^10.4.0",
    "puppeteer-extra": "^3.2.3",
    "puppeteer-extra-plugin-stealth": "^2.9.0"
  },
  "devDependencies": {
    "pkg": "^5.4.1"
  },
  "pkg": {
    "scripts": [
      "./bin/guardoni.js",
      "./src/domainSpecific.js",
      "./node_modules/**/*.js"
    ],
    "assets": [
      "extension/.keep",
      "screenshots/.keep",
      "experiments/.keep",
      "static/index.html",
      "config/default.json",
      "profiles/.keep"
    ],
    "targets": [
      "node14-linux-x64",
      "node14-win-x64",
      "node14-macos-x64"
    ],
    "outputPath": "dist"
  }
}
