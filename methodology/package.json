{
  "name": "guardoni",
  "version": "1.9.0",
  "description": "Guardoni is a tool to automate access and coordinate personalization algorithm analysis",
  "scripts": {
    "clean": "rm -rf build/**",
    "pkg": "pkg ./package.json && sh ./bin/post-compile.sh",
    "lint": "eslint ./src",
    "build": "webpack",
    "watch:main": "webpack watch",
    "watch:renderer": "webpack watch -c webpack.renderer.config.ts",
    "electron": "electron ./build/desktop/index.js"
  },
  "bin": "./build/guardoni-cli.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tracking-exposed/yttrex.git"
  },
  "keywords": [
    "youtube",
    "algorithm",
    "analysis"
  ],
  "author": "Claudio Agosti and the TRex team",
  "license": "AGPL-3.0-or-later",
  "homepage": "https://youtube.tracking.exposed/guardoni",
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "csv-parse": "^4.15.3",
    "debug": "^4.3.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "nconf": "^0.11.3",
    "node-fetch": "^2.6.6",
    "puppeteer": "^10.4.0",
    "puppeteer-core": "^13.0.0",
    "puppeteer-extra": "^3.2.3",
    "puppeteer-extra-plugin-stealth": "^2.9.0",
    "puppeteer-in-electron": "^3.0.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/eslint": "^8.2.1",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/webpack-node-externals": "^2.5.3",
    "bufferutil": "^4.0.5",
    "electron": "^16.0.5",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^8.4.1",
    "fsevents": "^2.3.2",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "pkg": "^5.4.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "utf-8-validate": "^5.0.7",
    "webpack": "^5.65.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-node-externals": "^3.0.0"
  },
  "pkg": {
    "scripts": [
      "./bin/guardoni.js",
      "./src/domainSpecific.js",
      "./node_modules/**/*.js"
    ],
    "assets": [
      "extension/.keep",
      "screenshots/.keep",
      "experiments/.keep",
      "static/index.html",
      "config/default.json",
      "profiles/.keep"
    ],
    "targets": [
      "node14-linux-x64",
      "node14-win-x64",
      "node14-macos-x64"
    ],
    "outputPath": "dist"
  }
}
